<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DashedLineView">
        <attr name="dlv_dashed_color" format="color"/>
        <attr name="dlv_solid_length" format="dimension"/>
        <attr name="dlv_gap_length" format="dimension"/>
        <attr name="dlv_stroke_width" format="dimension"/>
        <attr name="dlv_line_orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>
</resources>